{
    "version": 2,

    "buildCommand": "echo \"Skipping build on Vercel\"",
    "installCommand": "echo \"Skipping install on Vercel\"",

    "functions": {
      "api/index.php": {
        "runtime": "vercel-php@0.7.3"
      }
    },

    "routes": [
      {
        "src": "/build/(.*)",
        "dest": "/public/build/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
  }
